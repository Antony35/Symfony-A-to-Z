{% extends 'base.html.twig' %}

{% block title %}Hello PortfolioController!{% endblock %}

{% block body %}
<section>
    <h1>{{ categorie.nom }}</h1>
    <p>{{ categorie.description }}</p>
    {% for peinture in peintures %}
    <div> 
        <img src="{{ peinture.file }}" alt="">  
        <p>{{ peinture.nom }}</p>    
        <p>{{ peinture.description }}</p>            
    </div>
    {% endfor %}
</section>
{% endblock %}
 